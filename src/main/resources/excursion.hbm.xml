<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.softserve.edu.hibernate_task.entity.Excursion" table="excursion">
        <id name="excur_id" column="excur_id" type="java.lang.Integer">
            <generator class="increment"/> </id>
        <property name="name" column="name"
                  type="java.lang.String" />
        <property name="schedule" column="scheldule"
                  type="java.lang.String" />
        <property name="duration" column="duration"
                  type="java.lang.String" />
        <property name="resp_id" column="resp_id"
                  type="java.lang.Integer" />
    </class>
</hibernate-mapping>