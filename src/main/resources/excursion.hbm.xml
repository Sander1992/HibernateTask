<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.softserve.edu.hibernatetask.entity.Excursion" table="excursion">
        <id name="excurId" column="excur_id" type="java.lang.Integer">
            <generator class="increment"/> </id>
        <property name="name" column="name"
                  type="java.lang.String" />
        <property name="schedule" column="scheldule"
                  type="java.lang.String" />
        <property name="duration" column="duration"
                  type="java.lang.String" />
        <many-to-one name="fk_empl_id" class="com.softserve.edu.hibernatetask.entity.Employee" column="fk_empl_id"/>
    </class>
</hibernate-mapping>